   "source": [
    "public class Solution\n",
    "{\n",
    "    public string ReversePrefix(string word, char ch)\n",
    "        => string.Join(\"\", word.Substring(0, word.IndexOf(ch) + 1).Reverse()) + word.Substring(word.IndexOf(ch) + 1);\n",
    "}"
   ]
  }
